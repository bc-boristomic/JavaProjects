package ba.bitcamp.exercises.day2;

/**
 * Simulates Stack of Strings in LinkedList.
 * 
 * @author boris.tomic
 *
 */
public class StackOfLinkedStrings {

	private Node start;
	
	public StackOfLinkedStrings() {
		start = null;
	}
	
	private class Node {
		
		private Node next;
		private String value;
		
		public Node(String value) {
			this.value = value;
			next = null;
		}
		
		
	}
	
}
